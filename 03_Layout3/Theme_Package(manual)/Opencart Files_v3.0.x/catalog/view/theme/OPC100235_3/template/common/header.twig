{% if (column_left and column_right) %}
{% set layoutclass = 'layout-3' %}
{% elseif (column_left or column_right) %}
{% if column_left %}
{% set layoutclass = 'layout-2 left-col' %}
{% elseif column_right %}
{% set layoutclass = 'layout-2 right-col' %}
{% endif %}
{% else %}
{% set layoutclass = 'layout-1' %}
{% endif %}

<body class="{{ class }} {{ " " }} {{ layoutclass }}">
<nav id="top">
  <div class="container">
    <div class="row">
    
    
      {#}<div id="top-links" class="nav pull-right">
      <ul class="list-inline">
        {{ currency }}
        {{ language }}
		<li><a href="{{ contact }}"><i class="fa fa-phone"></i></a> <span class="hidden-xs hidden-sm hidden-md">{{ telephone }}</span></li>
       
        <li><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><i class="fa fa-heart"></i><span class="hidden-xs hidden-sm hidden-md">{{ text_wishlist }}</span></a></li>
        <li><a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_shopping_cart }}</span></a></li>
        <li><a href="{{ checkout }}"id="checkout-total" title="{{ text_checkout }}"><i class="fa fa-share"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_checkout }}</span></a></li>
      </ul>
    </div>#}
  </div>
  </div>
</nav>
<header>
<div class="header_top">
  <div class="container">
	<div class="row">
      <div class="col-sm-4 header-logo">
        <div id="logo">{% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>{% else %}
          <h1><a href="{{ home }}">{{ name }}</a></h1>
          {% endif %}</div>
      </div>  
	  {{ menu }}	
	  <div class="header_right">{{ headertop }}</div>
    </div>
  </div>
</div>

<div class="header_bottom">
	<div class="container">
		<div class="row">
			
			{{ headerleft }} 
		<div class="header_center">
			<div class="col-sm-5 header_search">{{ search }}</div>
		</div>			
	 <div class="account">			
       <li class="dropdown myaccount"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown">{#<i class="fa fa-user"></i> #}<span class="signin">{{ text_account_info }}</span><span class="hidden-xs hidden-sm hidden-md">{{ text_account }}</span> <span class="caret"></span></a>
          <ul class="dropdown-menu dropdown-menu-right myaccount-menu">
            <div class="drop_account">
				<div class="login_acc">
					<li><a class="login" href="{{ login }}">{{ text_login }}</a></li>
					<li><a class="reg" href="{{ register }}">{{ text_register }}</a></li>
					<li><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}">{{ text_wishlist }}</a></li>
					<li><a href="{{ checkout }}" title="{{ text_checkout }}"><span class="checkout">{{ text_checkout }}</span></a></li>
				 {% if logged %}
					<li><a class="account_logout1" href="{{ logout }}">{{ text_logout }}</a></li>
				{% endif %}
				</div>
			    <div class="lang-curr-wrapper">
					{{ language }}
					{{ currency }}
			    </div>
          </div>      
         </ul>
        </li>
      {#<div class="header_wishlist"><i class="fa fa-heart-o" aria-hidden="true"></i> </div>#}
      <div class="col-sm-3 header_cart">{{ cart }}</div>
    </div>
	</div>
     
    </div>
  </div>
